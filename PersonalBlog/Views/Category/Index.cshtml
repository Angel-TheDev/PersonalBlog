@model IEnumerable<PersonalBlog.Model.Category>

@{
    Layout = "/Views/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Category Post</title>
</head>
<body>
    <div>
        <h1>@category.Name</h1>
    </div>
    <main>

        @foreach (var post in Model)
        {

            <a href="/Movie/SingleMovie/@item.MovieId">
                <img src="~/Images/@item.ImageUrl" alt="Alternate Text" />
                <h3>@item.Title</h3>
            </a>
        }

    </main>
</body>
</html>

